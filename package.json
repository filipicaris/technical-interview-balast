{
  "name": "technical-interview-balast",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.txt dist",
    "format": "prettier --write . !**/*.{js,jsx,md,json}",
    "start": "npm run build && nest start",
    "start:debug": "npm run build && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --quiet",
    "test": "jest",
    "test:file": "jest --",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate": "db-migrate up -e default",
    "migrate:create": "db-migrate create $npm_config_name --sql-file -e default"
  },
  "dependencies": {
    "@kafkajs/confluent-schema-registry": "3.3.0",
    "@nestjs/axios": "3.1.3",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/schedule": "4.1.2",
    "@nestjs/swagger": "8.1.0",
    "@nestjs/terminus": "10.2.3",
    "@nestjs/typeorm": "10.0.2",
    "@scalar/nestjs-api-reference": "0.3.174",
    "@ssut/nestjs-sqs": "3.0.0",
    "@types/fuse": "^2.5.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "3.6.0",
    "dd-trace": "5.21.0",
    "deepmerge": "4.3.1",
    "env-var": "7.5.0",
    "fast-xml-parser": "^5.2.1",
    "fuse.js": "^7.1.0",
    "geolib": "3.3.4",
    "jose": "5.9.6",
    "kafkajs": "2.2.4",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.15",
    "@types/deepmerge": "2.2.0",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.5",
    "@types/sinon": "17.0.3",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "copyfiles": "^2.4.1",
    "db-migrate": "0.11.14",
    "db-migrate-pg": "1.5.2",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "node-mocks-http": "1.15.1",
    "prettier": "3.5.1",
    "sinon": "19.0.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}